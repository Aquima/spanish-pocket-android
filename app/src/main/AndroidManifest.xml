<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.quimalabs.sp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".IndoBannerActivity"></activity>
        <activity android:name=".Views.TestFutureActivity" />
        <activity android:name=".Views.TestImpPastActivity" />
        <activity android:name=".Views.TestSimplePastActivity" />
        <activity android:name=".Views.FutureActivity" />
        <activity android:name=".Views.ImpPastActivity" />
        <activity android:name=".Views.SimplePastActivity" />
        <activity android:name=".Views.TestPresentActivity" />
        <activity android:name=".Views.PresentActivity" />
        <activity android:name=".Views.ScoreActivity" />
        <activity android:name=".Views.RegisterActivity" />
        <activity android:name=".Views.LogInActivity" />
        <activity android:name=".Views.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
                android:name="preloaded_fonts"
                android:resource="@array/preloaded_fonts" />
    </application>

</manifest>